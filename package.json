{
   "name": "Shortcut-Visualizer",
   "private": true,
   "version": "0.0.1",
   "description": "Shortcut visualizer for creating/editing/visualizing keyboard shortcut layouts.",
   "main": "dist/index.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "build": "SET NODE_ENV=prod& webpack --display-error-details && npm run build.minify",
      "build.minify": "SET NODE_ENV=prod.min& webpack --display-error-details",
      "build:debug": "SET NODE_ENV=prod& node --inspect --debug-brk ./node_modules/webpack/bin/webpack.js",
      "dev": "SET NODE_ENV=dev& webpack-dev-server",
      "dev:extract": "SET NODE_ENV=dev.extract& webpack --display-error-details",
      "dev:debug": "SET NODE_ENV=dev& node --inspect --debug-brk ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "//note": "Don't add whitespace before &"
   },
   "author": "Alan <alanscodelog@gmail.com>",
   "license": "UNLICENSED",
   "dependencies": {},
   "devDependencies": {
      "babel-core": "^6.26.0",
      "babel-eslint": "^7.2.3",
      "babel-loader": "^7.1.2",
      "babel-plugin-transform-class-properties": "^6.24.1",
      "babel-plugin-transform-object-rest-spread": "^6.26.0",
      "babel-preset-env": "^1.6.0",
      "css-loader": "^0.28.7",
      "eslint": "^4.7.2",
      "eslint-loader": "^1.9.0",
      "eslint-plugin-snakecase": "0.0.1",
      "extract-text-webpack-plugin": "^3.0.0",
      "html-webpack-plugin": "^2.29.0",
      "node-sass": "^4.5.3",
      "sass-loader": "^6.0.6",
      "style-loader": "^0.18.2",
      "webpack": "^3.6.0",
      "webpack-dev-server": "^2.8.2"
   },
   "false": {}
}
